{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "%ModuleName%"
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  },
  "functionTimeout": "00:10:00" //https://docs.microsoft.com/en-us/azure/azure-functions/functions-host-json#functiontimeout
}